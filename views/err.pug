doctype html
html(lang='en')

  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(name='description' content=`${msg}: ${submsg}`)
    title= title
    link(rel='stylesheet' href=`/msg${min}.css`)

  body(data-gaid=gaid data-uid=uid data-event=event)
    h1= msg
    p= submsg
    script(async src='https://js.stripe.com/v3/')
    include _noscript
